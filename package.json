{
  "name": "package-age",
  "version": "0.4.1",
  "description": "A CLI for detecting old dependencies used in your project",
  "type": "module",
  "exports": "./index.js",
  "bin": {
    "package-age": "./cli.js"
  },
  "files": [
    "lib/",
    "cli.js",
    "index.js"
